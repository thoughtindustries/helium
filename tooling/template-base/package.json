{
  "name": "@thoughtindustries/helium-template",
  "version": "1.1.0-alpha.21",
  "scripts": {
    "authenticate": "helium authenticate",
    "deploy": "helium deploy",
    "dev": "helium dev",
    "update-translations": "helium update-translations",
    "dev:vite": "concurrently \"npm run dev:css\" \"npm run dev:server\"",
    "dev:css": "tailwindcss -o ./renderer/tailwind.css --watch",
    "dev:server": "tsup ./server/index.ts && node ./dist/index.js",
    "build": "npm run build:css && npm run build:vite && npm run build:worker",
    "build:css": "tailwindcss -o ./renderer/tailwind.css",
    "build:vite": "vite build && vite build --ssr",
    "build:worker": "wrangler build"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@apollo/client": "^3.5.4",
    "@cloudflare/kv-asset-handler": "^0.1.3",
    "@graphql-tools/graphql-tag-pluck": "^7.1.3",
    "@mdx-js/mdx": "^2.0.0-next.9",
    "@mdx-js/react": "^1.6.22",
    "@mdx-js/rollup": "^2.1.2",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@thoughtindustries/cart": "^1.1.1",
    "@thoughtindustries/catalog": "^1.1.4",
    "@thoughtindustries/content": "^1.1.0-alpha.3",
    "@thoughtindustries/featured-content": "^1.2.0-alpha.3",
    "@thoughtindustries/helium-server": "^1.2.0",
    "@thoughtindustries/hero": "^1.0.1-alpha.7",
    "@thoughtindustries/navigation-bar": "^1.1.1",
    "clsx": "^1.2.1",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "graphql": "^16.0.0",
    "i18next": "^21.4.2",
    "jwt-decode": "^3.1.2",
    "node-html-parser": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.14.2",
    "remark-frontmatter": "^4.0.1",
    "remark-mdx-frontmatter": "^2.0.3",
    "tailwindcss": "^3.0.9",
    "vite": "^2.5.2",
    "vite-plugin-ssr": "^0.3.59"
  },
  "devDependencies": {
    "@thoughtindustries/helium": "^1.1.0-alpha.6",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.39",
    "tsup": "^6.2.1",
    "typescript": "^4.5.5"
  },
  "volta": {
    "node": "14.17.6",
    "npm": "8.6.0"
  }
}
